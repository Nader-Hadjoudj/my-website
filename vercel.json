{
  "rewrites": [
    {
      "source": "/api/book-appointment/(.*)",
      "destination": "/api/book-appointments.js"
    },
    {
      "source": "/api/available-slots/(.*)",
      "destination": "/api/available-slots.js"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}